#include <iostream>
using namespace std;

//---------------------------------------------------------------------------------------- 
// Vertex Struct: Holds the name of the vertex, any other vertexes, or canals it's 
// 		  connected to and their corresponding weight (must be in order to work)
	
// Incoming Data: N/A

// Outgoing Data: N/A

// Author: Dakota Bell

// Tester: 

// Notes: 
	
// --------------------------------------------------------------------------- 

template <class T> 
struct Vertex
{
	T name; // hold name of the vertex
	T connection[9]; // hold up to 9 connecting vertexes
	T weights[9]; // hold up to 9 weights	
	T* link;
};
